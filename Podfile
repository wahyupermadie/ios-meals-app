# Uncomment the next line to define a global platform for your project
platform :ios, '13.0'
use_frameworks!
target 'TheMealsApp' do
  # Comment the next line if you don't want to use dynamic frameworks
  pod 'Alamofire'
  pod 'SDWebImageSwiftUI'
  pod 'RealmSwift'
  pod 'RxSwift', '~> 5'
  pod 'RxCocoa', '~> 5' 

  # Pods for TheMealsApp

  target 'TheMealsAppTests' do
    inherit! :search_paths
    pod 'RxBlocking', '~> 5' 
    pod 'RxTest', '~> 5'   
  end

  target 'TheMealsAppUITests' do
    pod 'RxBlocking', '~> 5' 
    pod 'RxTest', '~> 5' 
  end

end
